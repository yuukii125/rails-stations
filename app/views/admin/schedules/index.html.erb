<h1>上映時間一覧</h1>
<table>
  <tr>
    <th>ID</th>
    <th>タイトル</th>
    <th>作品ID</th>
    <th>開始時刻</th>
    <th>終了時刻</th>
    <th>登録日時</th>
    <th>更新日時</th>
  </tr>
  <% @schedules.each do |s| %>
    <tr>
      <td><%= s.id %></td>
      <td><%= s.movie.name %></td>
      <td><%= s.movie_id %></td>
      <td><%= link_to  "#{s.start_time}", admin_schedule_path(s.id) %></td>
      <td><%= link_to  "#{s.end_time}", admin_schedule_path(s.id) %></td>
      <td><%= s.created_at %></td>
      <td><%= s.updated_at %></td>
    </tr>
  <% end %>
</table>
